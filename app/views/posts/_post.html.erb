<div id="<%= dom_id post %>">
 

</div>
<table class="table table-striped">
  <thead>
    <tr>
    
      <th scope="col">Name</th>
      <th scope="col">Phone</th>
      <th scope="col">Location</th>
      <th scope="col">Date</th>
      <th scope="col">Worker Id</th>
      <th scope="col">Info</th>
      <th scope="col">Generated</th>
      <th scope="col">Coordinates</th>





    </tr>
  </thead>
  <tbody>
    <tr class='tablerow'>
      <td id='first'>
      <%= post.first_name%>
      <%= post.last_name %>
      </td>
     
      <td id='phone'><%= post.phone %></td>
      <td id='location'><%= post.location %></td>
      <td id='date'><%= post.date %></td>
      <td id='date'><%= post.user_id %></td>
      <td id='info'><%= link_to 'Info', post %></td>
      <td id='date'><%= post.created_at %></td>
      <td id='date'>
      <%= post.latitude %>
      <%=post.longitude%>
      </td>





    </tr>
  </tbody>
</table>
